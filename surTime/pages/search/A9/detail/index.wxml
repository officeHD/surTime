<view>
  <view class='tableInfo'>
    <view class='title'> 商品基础信息分析</view>
    <view class='infotop'>
      <view class='image'>
        <image src='{{tableInfo.ImageUrl}}'></image>
      </view>
      <view class='Product_title'>
        <view>Product Title:</view>
        {{tableInfo.ProductTitle}}
      </view>
    </view>
    <view class='infoList'>
      <view class='item'>
        <view class='item_list'>
          <view class='item_title'>Country:</view>
          <view class='content'>{{tableInfo.Country}}</view>
        </view>
        <view class='item_list'>
          <view class='item_title'>ASIN:</view>
          <view class='content'>{{tableInfo.ASIN}}</view>
        </view>

        <view class='item_list'>
          <view class='item_title'>UPC:</view>
          <view class='content'>{{tableInfo.UPC}}</view>
        </view>
        <view class='item_list'>
          <view class='item_title'>EAN:</view>
          <view class='content'>{{tableInfo.EAN}}</view>
        </view>
        <view class='item_list'>
          <view class='item_title'>Brand:</view>
          <view class='content'>{{tableInfo.Brand}}</view>
        </view>
        <view class='item_list'>
          <view class='item_title'>Model:</view>
          <view class='content'>{{tableInfo.Model}}</view>
        </view>
        <view class='item_list'>
          <view class='item_title'>MPN:</view>
          <view class='content'>{{tableInfo.MPN}}</view>
        </view>
        <view class='item_list'>
          <view class='item_title'>Review:</view>
          <view class='content'>{{tableInfo.Review}}</view>
        </view>
        <view class='item_list'>
          <view class='item_title'>Manufacture:</view>
          <view class='content'>{{tableInfo.Manufacture}}</view>
        </view>
        <view class='item_list'>
          <view class='item_title'>PartNumber:</view>
          <view class='content'>{{tableInfo.PartNumber}}</view>
        </view>
        <view class='item_list'>
          <view class='item_title'>Department:</view>
          <view class='content'>{{tableInfo.Department}}</view>
        </view>
        <view class='item_list'>
          <view class='item_title'>ReviewLevel:</view>
          <view class='content'>{{tableInfo.ReviewLevel}}</view>
        </view>
      </view>
    </view>
    <view class='search'>
      <view class='pick_view'>
        <view class='status {{active==="SearchTerms"?"active":""}}' data-target="SearchTerms" bindtap='searchType'>
          SearchTerms
        </view>
        <view class='status {{active==="Features"?"active":""}}' data-target="Features" bindtap='searchType'>
          Features
        </view>
        <view class='status {{active==="Description"?"active":""}}' data-target="Description" bindtap='searchType'>
          Description
        </view>
      </view>
    </view>
    <view class='detail'>
      <view wx-if="{{active==='Description'}}"> {{tableInfo.Description}}</view>
      <view wx-if="{{active==='SearchTerms'}}"> {{tableInfo.SearchTerms}}</view>
      <view wx-if="{{active==='Features'}}"> {{tableInfo.Features}}</view>
    </view>
  </view>
  <view class='tableInfo'>
    <view class='title'> 变体所有信息</view>
    <view class="table">
      <view class='thead'>
        <view class="tr">
          <view class="th">Number</view>
          <view class="th">ASIN </view>
          <view class="th">Parentage </view>
          <view class="th"></view>
        </view>
      </view>
      <view class='tbody'>
        <block wx:for="{{PIChild}}" wx:key="{{index}}">
          <view class="tr">
            <view class="td">{{index}} </view>
            <view class="td">{{item.ASIN}}</view>
            <view class="td">{{item.Parentage}}</view>
            <view class="td">

              <image src='/images/table/detail.png' data-url='{{item.ASINLink}}' bindtap='viewdetail'></image>


            </view>
          </view>
        </block>
      </view>
    </view>
  </view>
  <view class='tableInfo'>
    <view class='title'>
      <image wx-if='{{chartIndex>1}}' bindtap='prevChart' class='controll pl' src='/images/arrow_left.png'></image>

      <text wx-if="{{chartIndex===1}}">近三个月曝光量、点击量、销量明细</text>
      <text wx-if="{{chartIndex===2}}">卖家流量渠道占比</text>
      <text wx-if="{{chartIndex===3}}">关键词入口渠道占比</text>
      <image wx-if='{{chartIndex<3}}' bindtap='nextChart' class='controll pr' src='/images/arrow_right.png'></image>
    </view>
    <view class='echart'>
      <ec-canvas id="mychart-dom-bar" canvas-id="mychart-bar" ec="{{ ec }}"></ec-canvas>

    </view>
    <view class="table" wx-if="{{chartIndex===2&&SourcesStatistics.length>0}}">
      <view class='thead'>
        <view class="tr">
          <view class="th">店铺名称</view>
          <view class="th">销售额度占比 </view>

        </view>
      </view>
      <view class='tbody'>
        <block wx:for="{{SourcesStatistics}}" wx:key="{{index}}">
          <view class="tr">
            <view class="td">{{item.StoreName}} </view>
            <view class="td">{{item.StoreNameRate}}</view>
          </view>
        </block>
      </view>
    </view>
  </view>
  <view class='tableInfo'>
    <view class='title'>
      <image wx-if='{{tableIndex>1}}' bindtap='prevChart' class='controll pl' src='/images/arrow_left.png'></image>

      <text wx-if="{{tableIndex===1}}">精准关键词</text>
      <text wx-if="{{tableIndex===2}}">流量关键词</text>
      <text wx-if="{{tableIndex===3}}">关联ASIN分析</text>
      <image wx-if='{{tableIndex<3}}' bindtap='nextChart' class='controll pr' src='/images/arrow_right.png'></image>

    </view>
    <view class="table" wx-if="{{tableIndex===1}}">
      <view class='thead'>
        <view class="tr">
          <view class="th">店铺名称</view>
          <view class="th">销售额度占比 </view>

        </view>
      </view>
      <view class='tbody'>
        <block wx:for="{{SourcesStatistics}}" wx:key="{{index}}">
          <view class="tr">
            <view class="td">{{item.StoreName}} </view>
            <view class="td">{{item.StoreNameRate}}</view>
          </view>
        </block>
      </view>
    </view>
    <view class="table" wx-if="{{tableIndex===2}}">
      <view class='thead'>
        <view class="tr">
          <view class="th">店铺名称</view>
          <view class="th">销售额度占比 </view>

        </view>
      </view>
      <view class='tbody'>
        <block wx:for="{{SourcesStatistics}}" wx:key="{{index}}">
          <view class="tr">
            <view class="td">{{item.StoreName}} </view>
            <view class="td">{{item.StoreNameRate}}</view>
          </view>
        </block>
      </view>
    </view>
    <view class="table" wx-if="{{tableIndex===3}}">
      <view class='thead'>
        <view class="tr">
          <view class="th">店铺名称</view>
          <view class="th">销售额度占比 </view>

        </view>
      </view>
      <view class='tbody'>
        <block wx:for="{{SourcesStatistics}}" wx:key="{{index}}">
          <view class="tr">
            <view class="td">{{item.StoreName}} </view>
            <view class="td">{{item.StoreNameRate}}</view>
          </view>
        </block>
      </view>
    </view>
  </view>



  <view>ASINLink:{{tableInfo.ASINLink}}</view>
  <view>PKID:{{tableInfo.PKID}}</view>
  <view>Parentage:{{tableInfo.Parentage}}</view>
  <view>FK_PIASIN:{{tableInfo.FK_PIASIN}}</view>
</view>