<view class="mainNav">
  <view wx:for="{{navbar}}" wx:key="{{index}}" class='item'>
    <view class='title_name'>{{item.name}}</view>
    <view class='subNav'>
      <block wx:for="{{item.navgator}}" wx:key="{{index}}">
        <view class='list_url' bindtap='toPage' data-url='{{item.url}}'>{{item.name}}</view>
      </block>
    </view>
  </view>
</view>